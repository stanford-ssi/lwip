{
  "name": "lwip",
  "version": "99",
  "build" : {
    "srcDir": ".",
    "srcFilter": [
      "+<*>",
      "-<doc>",
      "-<test>",
      "-<src/apps>",
      "-<contrib>",
      "+<contrib/ports/freertos>",
      "-<src/netif>",
      "+<src/netif/ethernet.c>"
    ],
    "flags": [
      "-I src/include/",
      "-I src/include/netif/",
      "-I contrib/ports/freertos/include/",
      "-D LWIP_COMPAT_MUTEX_ALLOWED"
    ]
  }
}
